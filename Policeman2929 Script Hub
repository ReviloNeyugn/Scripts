local Section = SAUTTab:CreateSection("Coins")

local Button = SAUTTab:CreateButton({
   Name = "Collect All Coins",
   Callback = function()
      local coinsFolder = workspace:FindFirstChild("Coins2")
      if coinsFolder then
         local hrp = game.Players.LocalPlayer.Character.HumanoidRootPart
         if hrp then
            local OriginalCFrame = hrp.CFrame
            for _, coin in pairs(coinsFolder:GetChildren()) do
               if coin.Name == "Union" then
                  -- Teleport to coin
                  hrp.CFrame = coin.CFrame
                  task.wait(0.2) -- short delay so it visits them one by one
                  hrp.CFrame = OriginalCFrame
               end
            end
         end
      end
   end,
})


local CtoggleEnabled = false

local Toggle = SAUTTab:CreateToggle({
   Name = "Auto Collect Coins",
   CurrentValue = false,
   Flag = "CoinToggle",
   Callback = function(Value)
      CtoggleEnabled = Value

      if CtoggleEnabled then
         task.spawn(function()
            while CtoggleEnabled do
               local coinsFolder = workspace:FindFirstChild("Coins2")
               if coinsFolder then
                  for _, coin in pairs(coinsFolder:GetChildren()) do
                     if coin.Name == "Union" then
                        local hrp = game.Players.LocalPlayer.Character.HumanoidRootPart
                        local OriginalCFrame = hrp.CFrame
                        if hrp then
                           hrp.CFrame = coin.CFrame
                           task.wait(0.2) -- short delay between teleports
                           hrp.CFrame = OriginalCFrame
                        end
                     end
                  end
               end

               task.wait(1) -- check again every second for new coins
            end
         end)
      end
   end,
})

local NCtoggleEnabled = false

local Toggle = SAUTTab:CreateToggle({
   Name = "Notify When Coins Spawn",
   CurrentValue = false,
   Flag = "CoinFolderNotifyToggle",
   Callback = function(Value)
      NCtoggleEnabled = Value

      if NCtoggleEnabled then
         task.spawn(function()
            -- Watch workspace for new children
            workspace.ChildAdded:Connect(function(child)
               if NCtoggleEnabled and child.Name == "Coins2" then
                  Rayfield:Notify({
                     Title = "Coins Spawned!",
                     Content = "Coins have spawned. You can teleport to all the coins in the GUI.",
                     Duration = 5, -- seconds
                     Image = nil, -- optional image ID
                  })
               end
            end)

            -- Safety loop so the toggle can be turned off cleanly
            while toggleEnabled do
               task.wait(1)
            end
         end)
      end
   end,
})


